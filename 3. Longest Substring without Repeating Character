class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        dic={}
        tlen=0
        Mlen=0
        bindex=0
        eindex=0
        for a in range(0,len(s),1):
            print('start of',a)
            if dic.get(s[a])==None:
                dic[s[a]]=a
                tlen+=1
                if tlen>Mlen:
                    Mlen=tlen
            else:
                eindex=dic.get(s[a])
                for b in range(bindex,eindex+1,1):
                    del dic[s[b]]
                    tlen-=1
                    bindex+=1
                dic[s[a]]=a
                tlen+=1
        return Mlen
                
